<?xml version="1.0" encoding="UTF-8"?>
<Policy xmlns="urn:oasis:names:tc:xacml:2.0:policy:schema:os" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PolicyId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIB043:policy" RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:deny-overrides" xsi:schemaLocation="urn:oasis:names:tc:xacml:2.0:policy:schema:os         access_control-xacml-2.0-policy-schema-os.xsd">
    <Description>
        Policy for Conformance Test IIB043.
    </Description>
    <Target/>
    <Rule Effect="Permit" RuleId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIB043:rule">
        <Description>
            Julius Hibbert can read or write Bart Simpson's
            medical record if that record is authored by
            Julius Hibbert.
        </Description>
        <Target><Subjects><Subject><SubjectMatch MatchId="urn:oasis:names:tc:xacml:1.0:function:string-equal"><AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">RandomValue_##+]][[*##_####$%098763524_RandomValue456Mutant_xyz</AttributeValue><SubjectAttributeDesignator AttributeId="it.cnr.isti.sedc.RandomAttributeId.subject:subject-id" DataType="http://www.w3.org/2001/XMLSchema#string"/></SubjectMatch></Subject></Subjects></Target>
        <Condition>
<Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:string-equal">
            <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:string-one-and-only">
                <ResourceAttributeDesignator AttributeId="urn:oasis:names:tc:xacml:2.0:conformance-test:author" DataType="http://www.w3.org/2001/XMLSchema#string"/>
            </Apply>
            <AttributeValue DataType="http://www.w3.org/2001/XMLSchema#string">Bartholomew Simpson</AttributeValue>
        
</Apply></Condition>
    </Rule>
</Policy>
